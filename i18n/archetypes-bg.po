#. <input class="context" i18n:attributes="value label_next;" name="form_next" tabindex="" tal:attributes="tabindex tabindex/next;                                        disabled python:test(isLocked, 'disabled', None);" tal:condition="python:fieldset_index &lt; n_fieldsets - 1" type="submit" value="Next"/>
#: ./archetypes/edit_macros.pt
msgid "label_next"
msgstr "Следващ"

# Original: "Previous"
#. <input class="context" i18n:attributes="value label_previous;" name="form_previous" tabindex="" tal:attributes="tabindex tabindex/next;                                        disabled python:test(isLocked, 'disabled', None);" tal:condition="python:fieldset_index &gt; 0" type="submit" value="Previous"/>
#: ./archetypes/edit_macros.pt
msgid "label_previous"
